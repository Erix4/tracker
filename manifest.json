{
    "name": "Tracker",
    "description": "Use to track shows you're watching!",
    "version": "0.0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "tabs",
        "downloads"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/assets/trackerIcon16.png",
            "32": "/assets/trackerIcon32.png",
            "64": "/assets/trackerIcon64.png",
            "129": "/assets/trackerIcon128.png"
        }
    },
    "icons": {
        "16": "/assets/trackerIcon16.png",
        "32": "/assets/trackerIcon32.png",
        "64": "/assets/trackerIcon64.png",
        "129": "/assets/trackerIcon128.png"
    }
}